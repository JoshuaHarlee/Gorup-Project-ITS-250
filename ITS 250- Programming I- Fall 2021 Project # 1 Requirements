age = 100
ASVAB_score = 99
good_credit = 650
bad_credit = 649

def main():
    first_name = input('Enter your first name: ')
    last_name = input('Enter your last name: ')
    current_education = input('Enter your Ed: ')
    height = input('Enter your height in inches: ')
    weight = input('Enter your weight: ')
    disqualifiers ()

 
def disqualifiers():
    age = int(input('Enter your age: ')) 
    if age <= 42:
        print (' Qualified for age ')
    elif age >= 42:
        print ('Disqualified for age')
    disqualifiers2()


def disqualifiers2():
    ASVAB_score = int(input('Enter ASVAB score: '))
    if ASVAB_score >= 31:
        print ('Qualified ASVAB Score')
    elif ASVAB_score < 31:
        print ('Disqualified ASVAB Score ')
    disqualifier3()

def disqualifier3():
    credit_score = int(input("Enter your Credit Score: "))
    while good_credit >= 650:
        if credit_score >= good_credit:
                print ('Your Credit Score is good') 
        if credit_score < good_credit: 
                print ('Your Credit Score is bad') 
        break  



main ()
